<div class="container">
	<h1>Strategy Formation</h1>
	<hr>
	<div class="row">
		<div class="col-md-4">
			<h5>Stock</h5>
			<input type="text" id="stocknum" class="form-control" />
		</div>
		<div class="col-md-4">
			<h5>Time From</h5>
			<input type="date" id="dayfrom" class="form-control">
		</div>
		<div class="col-md-4">
			<h5>Time To</h5>
			<input type="date" id="dayto" class="form-control">
		</div>
	</div>
	<br>
	<pre id="console">Console: Please enter your stock...</pre>
	<hr>
	<div class="row">
	  <div class="col-xs-12 col-md-8">
		<h5>Indicator</h5>
		<textarea id="indicatordisplay" class="form-control" rows=4></textarea>
		<% _.each(indicators, function(indicator) { %>
		<p id="indicator_<%= indicator.name %>" hidden><%= indicator.code %></p>
		<% }) %>
	  </div>
	  <div class="col-xs-6 col-md-4">
		<h5>Library</h5>
		
		<select class="form-control" id="indicatorselect">
		  <option></option>
		  <% _.each(indicators, function(indicator) { %>
		  <option><%= indicator.name %></option>
		  <% }) %>
		</select>
		<br>
	  	<button type="submit" id="import" class="btn btn-success btn-block">Import</button>
	  </div>
	</div>
	<br>
  	<pre id="console2">Console: Please import indicators...</pre>
	<hr>
	<h5>Strategy</h5>
	<textarea id="code" class="form-control" rows=6>
initial_capital = 100000;
cost = 5 + 1000 * 0.05;

buy = mov_avg(t, 30) < mov_avg(t, 50);
sell = mov_avg(t, 30) > mov_avg(t, 15);

num_of_shares = 1000/data[t].Close;

buy_amt = num_of_shares;
sell_amt = num_of_shares;</textarea>
	<hr>
  	<pre id="console3">Console: Please import the stock and indicator first...</pre>
	<input type="submit" id="run" class="btn btn-lg btn-primary btn-block" value="Test" />
	<input type="hidden" name="_csrf" value="<%= _csrf %>" />
</div>